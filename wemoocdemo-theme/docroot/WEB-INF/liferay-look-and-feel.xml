<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.0.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_0_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.1.1+</version>
	</compatibility>
	<theme id="wemoocdemo" name="Wemoocdemo">
		<color-scheme id="01" name="guest">
			<css-class>guest</css-class>
			<color-scheme-images-path>${images-path}/custom/color_schemes/${css-class}</color-scheme-images-path>
		</color-scheme>
		<color-scheme id="02" name="pop_up">
			<css-class>pop_up_scheme</css-class>
			<color-scheme-images-path>${images-path}/custom/color_schemes/${css-class}</color-scheme-images-path>
		</color-scheme>
	</theme>			
</look-and-feel>