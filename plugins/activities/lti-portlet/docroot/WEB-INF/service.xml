<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_0_0.dtd">
<service-builder package-path="com.liferay.lms.lti">
	<author>Diego Renedo Delgado</author>
	<namespace>lti</namespace>
	<entity remote-service="false" uuid="true" local-service="true" name="LtiItem" table="lti_item" json-enabled="false">
	
		<!-- PK fields -->
		<column name="ltiItemId" type="long" primary="true" />
		
		<!-- FK fields -->
		<column name="actId" type="long" />		

		<!-- Audit fields -->
		<column name="companyId" type="long" />
		<column name="groupId" type="long" />
		<column name="createDate" type="Date" />
		<column name="userId" type="long" />
		
		<!-- Other fields -->
		<column name="name" type="String" />
		<column name="description" type="String" />
		<column name="url" type="String" />
		<column name="secret" type="String" />
		<column name="rol" type="String" />
		<column name="contenType" type="String" />
		<column name="iframe" type="boolean" />
		<column name="note" type="int" />
		
		<finder name="actId" return-type="LtiItem">
			<finder-column name="actId" />
		</finder>
	</entity>
</service-builder>