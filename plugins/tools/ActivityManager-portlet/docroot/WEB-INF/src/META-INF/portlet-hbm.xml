<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.liferay.lmssa.model.ActManAudit" />
	<import class="com.liferay.lmssa.model.LearningActivityTryDeleted" />
	<class name="com.liferay.lmssa.model.impl.ActManAuditImpl" table="lmssa_ActManAudit">
		<cache usage="read-write" />
		<id name="actManAuditId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="courseId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="className" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="start" type="org.hibernate.type.TimestampType" column="start_" />
		<property name="end" type="org.hibernate.type.TimestampType" column="end_" />
		<property name="state" type="com.liferay.portal.dao.orm.hibernate.StringType" column="state_" />
		<property name="number" type="com.liferay.portal.dao.orm.hibernate.LongType" column="number_" />
	</class>
	<class name="com.liferay.lmssa.model.impl.LearningActivityTryDeletedImpl" table="lmssa_LearningActivityTryDeleted">
		<cache usage="read-write" />
		<id name="latDelId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="actManAuditId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="latId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="actId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="startDate" type="org.hibernate.type.TimestampType" />
		<property name="result" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="endDate" type="org.hibernate.type.TimestampType" />
		<property name="tryData" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="tryResultData" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="comments" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>